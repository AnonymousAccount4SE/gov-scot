{
  "name": "gov-scot",
  "description": "Build environment for the gov.scot web site",
  "version": "0.0.0",
  "author": {
    "name": "mygov.scot team",
    "email": "dleepdigmsgb@scotland.gsi.gov.uk",
    "url": "http://www.gov.scot/"
  },
  "scripts": {
    "postshrinkwrap": ".bin/sealant @scottish-government/mygov-assets",
