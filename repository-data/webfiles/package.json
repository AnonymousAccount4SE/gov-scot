  },
  "devDependencies": {
    "@scottish-government/mygov-assets": "latest",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "html5shiv": "^3.7.3",
    "jquery": "^1.12.4",
    "pikaday": "^1.4.0",
    "sealant": "0.0.2",
    "svg-sprite": "^1.4.0",
    "webpack": "^1.13.1"
  }
}
